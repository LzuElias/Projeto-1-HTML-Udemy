        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "xa=4\n",
        "x1a=np.array([xa])\n",
        "na=20\n",
        "aa=5\n",
        "ca=0\n",
        "ma=7\n",
        "for ia in range(na):\n",
        "    xa=(aa*xa+ca)%ma\n",
        "    x1a=np.append(x1a,xa)\n",
        "print(x1a)\n",
        "inda=np.arange(na+1)\n",
        "plt.bar(inda, x1a)\n",
        "plt.show()\n",
